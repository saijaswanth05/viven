#include <stdio.h>
int main(){
    int a[50],b[50],n1,n2,i,j,k=0;
    int un[100];
    int exists;
    printf("enter 1st array:");
    scanf("%d",&n1);
    for(i=0;i<n1;i++){
        scanf("%d",&a[i]);
    }
    printf("enter 2nd array:");
    scanf("%d",&n2);
    for(i=0;i<n2;i++){
        scanf("%d",&b[i]);
    }
    //for union
    for(i=0;i<n1;i++){
        un[k++]=a[i];
    }
    for(i=0;i<n2;i++){
        exists=0;
        for(j=0;j<k;j++){
            if(b[i]==un[j]){
                exists=1;
                break;
            }
        }
        if (!exists){
            un[k++]=b[i];
        }
    }
    printf("union array is:");
    for(i=0;i<k;i++){
        printf("%d  ",un[i]);
    }

    return 0;
}
