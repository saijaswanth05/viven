#include <stdio.h>
#define max 100
    
int main() {
    int i,j,arr[max],n,temp;
    printf("enter array size:");
    scanf("%d",&n);
    printf("enter array:");
    for(i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    //bubble sort
    for(i=0;i<n-1;i++){
        for(j=0;j<n-1-i;j++){
            
            if(arr[j]<arr[j+1]){
                temp=arr[j];
                arr[j]=arr[j+1];
                arr[j+1]=temp;
            }
        }
    }
    printf("array after sorted:\n");
    for(i=0;i<n;i++){
        printf("%d  ",arr[i]);
    }
    return 0;
}
